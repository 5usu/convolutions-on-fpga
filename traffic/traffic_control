#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x6154f486c410 .scope module, "traffic_tb" "traffic_tb" 2 3;
 .timescale -9 -9;
v0x6154f4882970_0 .var "clk", 0 0;
v0x6154f4882a10_0 .net "light", 1 0, v0x6154f4853eb0_0;  1 drivers
v0x6154f4882ae0_0 .var "reset", 0 0;
E_0x6154f48675c0 .event posedge, v0x6154f4853db0_0;
S_0x6154f486c5a0 .scope module, "uut" "traffic_control" 2 8, 3 1 0, S_0x6154f486c410;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 2 "light";
P_0x6154f4858f80 .param/l "GREEN" 0 3 10, C4<10>;
P_0x6154f4858fc0 .param/l "GREEN_TIME" 0 3 14, +C4<00000000000000000000000000001010>;
P_0x6154f4859000 .param/l "RED" 0 3 8, C4<00>;
P_0x6154f4859040 .param/l "RED_TIME" 0 3 12, +C4<00000000000000000000000000001010>;
P_0x6154f4859080 .param/l "YELLOW" 0 3 9, C4<01>;
P_0x6154f48590c0 .param/l "YELLOW_TIME" 0 3 13, +C4<00000000000000000000000000001010>;
v0x6154f4853db0_0 .net "clk", 0 0, v0x6154f4882970_0;  1 drivers
v0x6154f4853eb0_0 .var "light", 1 0;
v0x6154f4882530_0 .var "next_state", 1 0;
v0x6154f4882620_0 .net "reset", 0 0, v0x6154f4882ae0_0;  1 drivers
v0x6154f48826e0_0 .var "state", 1 0;
v0x6154f4882810_0 .var "timer", 5 0;
E_0x6154f4867aa0 .event edge, v0x6154f48826e0_0;
E_0x6154f48519b0 .event posedge, v0x6154f4882620_0, v0x6154f4853db0_0;
E_0x6154f48579b0 .event edge, v0x6154f48826e0_0, v0x6154f4882810_0;
    .scope S_0x6154f486c5a0;
T_0 ;
    %wait E_0x6154f48519b0;
    %load/vec4 v0x6154f4882620_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x6154f48826e0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x6154f4882530_0;
    %assign/vec4 v0x6154f48826e0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x6154f486c5a0;
T_1 ;
    %wait E_0x6154f48579b0;
    %load/vec4 v0x6154f48826e0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x6154f4882530_0, 0, 2;
    %jmp T_1.4;
T_1.0 ;
    %load/vec4 v0x6154f4882810_0;
    %pad/u 32;
    %cmpi/e 9, 0, 32;
    %jmp/0xz  T_1.5, 4;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x6154f4882530_0, 0, 2;
    %jmp T_1.6;
T_1.5 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x6154f4882530_0, 0, 2;
T_1.6 ;
    %jmp T_1.4;
T_1.1 ;
    %load/vec4 v0x6154f4882810_0;
    %pad/u 32;
    %cmpi/e 9, 0, 32;
    %jmp/0xz  T_1.7, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x6154f4882530_0, 0, 2;
    %jmp T_1.8;
T_1.7 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x6154f4882530_0, 0, 2;
T_1.8 ;
    %jmp T_1.4;
T_1.2 ;
    %load/vec4 v0x6154f4882810_0;
    %pad/u 32;
    %cmpi/e 9, 0, 32;
    %jmp/0xz  T_1.9, 4;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x6154f4882530_0, 0, 2;
    %jmp T_1.10;
T_1.9 ;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x6154f4882530_0, 0, 2;
T_1.10 ;
    %jmp T_1.4;
T_1.4 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x6154f486c5a0;
T_2 ;
    %wait E_0x6154f48519b0;
    %load/vec4 v0x6154f4882620_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x6154f4882810_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x6154f48826e0_0;
    %pushi/vec4 0, 0, 2;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x6154f4882810_0;
    %pad/u 32;
    %pushi/vec4 9, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %load/vec4 v0x6154f48826e0_0;
    %pushi/vec4 1, 0, 2;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x6154f4882810_0;
    %pad/u 32;
    %pushi/vec4 9, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 9;
    %flag_or 9, 8;
    %load/vec4 v0x6154f48826e0_0;
    %pushi/vec4 2, 0, 2;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x6154f4882810_0;
    %pad/u 32;
    %pushi/vec4 9, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %flag_or 8, 9;
    %jmp/0xz  T_2.2, 8;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x6154f4882810_0, 0;
    %jmp T_2.3;
T_2.2 ;
    %load/vec4 v0x6154f4882810_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x6154f4882810_0, 0;
T_2.3 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x6154f486c5a0;
T_3 ;
    %wait E_0x6154f4867aa0;
    %load/vec4 v0x6154f48826e0_0;
    %store/vec4 v0x6154f4853eb0_0, 0, 2;
    %jmp T_3;
    .thread T_3, $push;
    .scope S_0x6154f486c410;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6154f4882970_0, 0, 1;
T_4.0 ;
    %delay 5, 0;
    %load/vec4 v0x6154f4882970_0;
    %inv;
    %store/vec4 v0x6154f4882970_0, 0, 1;
    %jmp T_4.0;
    %end;
    .thread T_4;
    .scope S_0x6154f486c410;
T_5 ;
    %vpi_call 2 20 "$dumpfile", "traffic_control.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x6154f486c410 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6154f4882ae0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6154f4882ae0_0, 0, 1;
    %delay 1000, 0;
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_5;
    .scope S_0x6154f486c410;
T_6 ;
    %wait E_0x6154f48675c0;
    %vpi_call 2 32 "$display", "Time=%0t, Light=%b", $time, v0x6154f4882a10_0 {0 0 0};
    %jmp T_6;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
